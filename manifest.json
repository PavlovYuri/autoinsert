{
    "manifest_version": 3, 
    "name": "AutoInsert", 
    "version": "1.0.0",
    "description": "Automatic search for the answer to the question and its insertion",
    "icons": {
        "48": "images/icon-48.png", 
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "http://localhost:7000/objects"
    ]
}